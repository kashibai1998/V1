        supplierDto.faxNumber( supplierPrimaryContactFax( supplier ) );
        supplierDto.supplierAddress( addressToAddressDto( supplier.getSupplierAddress() ) );
        supplierDto.supplierBank( supplierBankInfoToBankInfoDto( supplier.getSupplierBank() ) );
        supplierDto.supplierId( supplier.getSupplierId() );
        supplierDto.supplierType( supplier.getSupplierType() );

        return supplierDto.build();
